#!/usr/bin/python

import eyeD3, os, shutil

tag = eyeD3.Tag()

mp3s = os.listdir('./')
for mp3 in mp3s:
    tag.link('./' + mp3)
    if mp3.endswith('mp3'):
        albumdir = tag.getYear() + '_-_' + tag.getAlbum().replace(' ', '_').replace(':', '-')
        os.mkdir('./' + albumdir)
        shutil.move(mp3, albumdir)
