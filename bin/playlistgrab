#!/usr/bin/python

import os
import shutil
import sys

tfn = ""

filepath = sys.argv[1]
t = filepath.split('.')
f = open(filepath, 'r')
os.mkdir(t[0])
for line in f:
	line = line.replace("\n","")
	if line[0] == '#':
		temp = line.split(',')
		tfn = temp[1].replace(' ', '_').replace("/","-")
	else:
		#print os.path.exists(line)
		shutil.copy(line, './'+ t[0] +'/' + tfn + '.mp3')
		#print tfn
